<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="Python FastCGI" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\Python311\python.exe|C:\Python311\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
    </system.webServer>

    <appSettings>
        <add key="PYTHONPATH" value="C:\inetpub\wwwroot\api" />
        <add key="WSGI_HANDLER" value="ecommerce.wsgi.application" />
        <add key="DJANGO_SETTINGS_MODULE" value="ecommerce.settings" />
    </appSettings>
</configuration>